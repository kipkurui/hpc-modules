# for Tcl script use only
set     appname         cytoscape
set     version         3.1.1
set     prefix          /opt/apps/${appname}/${version}
set     exec_prefix     ${prefix}

conflict        cytoscape

prepend-path    PATH            ${exec_prefix}

# depends on biana
if { ![is-loaded biana] } {
        module load biana/everlyn
}
